import os
from os import listdir
from os.path import isfile, join
import shutil
import zipfile

def unzip_files(source_dir, dst_dir):
    for root, dirs, files in os.walk(os.path.abspath(source_dir)):
        for file in files:
            if not str(file).endswith("zip"):
                continue
            full_path = os.path.join(root, file)
            with zipfile.ZipFile(full_path,'r') as zFile:
                zFile.extractall(dst_dir)
            print("unzip: " + str(file))

target_dir = "D:\\1.1\\"
source_path = "D:\\1\\"
unzip_files(source_path, target_dir)